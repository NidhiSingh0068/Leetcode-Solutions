class Solution:
    def hasEvenDigits(self, num) -> int:
        count = 0
        while num != 0:
            num //= 10
            count += 1
        if (count%2==0):
            return True
        else:
            return False
    
    def findNumbers(self, nums: List[int]) -> int:
        evencount = 0
        for num in nums:
            if self.hasEvenDigits(num):
                evencount += 1
        return evencount
